{
  "name": "owenfly",
  "description": "owenfly",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenfly1/owenfly"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build:before": "node ./scripts/before_build.js",
    "build:dev": "npm run build:before && cross-env REACT_APP_ENV=dev UMI_ENV=dev umi build && npm run build:widget:dev",
    "build:test": "npm run build:before && cross-env REACT_APP_ENV=test UMI_ENV=test umi build && npm run build:widget:test",
    "build:pre": "npm run build:before && cross-env REACT_APP_ENV=pre UMI_ENV=pre umi build && npm run build:widget:pre",
    "build:prod": "npm run build:before && cross-env REACT_APP_ENV=prod UMI_ENV=prod umi build && npm run build:widget:prod",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "playwright": "playwright install && playwright test",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=test umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre MOCK=none UMI_ENV=pre umi dev",
    "start:prod": "cross-env REACT_APP_ENV=prod MOCK=none UMI_ENV=prod umi dev",
    "test": "npm run start:test",
    "pre": "npm run start:pre",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit",
    "build:widget:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=widget HTML=none umi build && node ./scripts/widget.js",
    "build:widget:test": "cross-env REACT_APP_ENV=test UMI_ENV=widget HTML=none umi build && node ./scripts/widget.js",
    "build:widget:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=widget HTML=none umi build && node ./scripts/widget.js",
    "build:widget:prod": "cross-env REACT_APP_ENV=prod UMI_ENV=widget HTML=none umi build && node ./scripts/widget.js"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^0.9.4",
    "@ant-design/icons": "^4.7.0",
